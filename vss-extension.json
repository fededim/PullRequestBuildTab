{
    "manifestVersion": 1,
    "id": "pr-tabs-build",
    "publisher": "fededim",
    "version": "0.0.3",
    "baseUri": "https://localhost:3000",
    "name": "Pull Request Builds tab",
    "description": "An extensions to add a \"Builds\" tab to pull requests showing all the related builds",
    "public": false,
    "categories": ["Azure Repos"],
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services"
        }
    ],
    "contributions": [
        {
            "id": "pr-tabs-build",
            "type": "ms.vss-web.tab",
            "targets": [
                "ms.vss-code-web.pr-tabs"
            ],
            "properties": {
                "name": "Builds",
                "uri": "pr-tabs-build/pr-tabs-build.html"
            }
        }
    ],
	"demands": [
        "api-version/3.0"
    ],
	"scopes": [],
	"icons": {
        "default": "static/images/logo.png"
    },
	"files": [
		{
			"path": "static",
			"addressable": true
		},
        {
            "path": "dist",
            "addressable": true
        },
        {
            "path": "node_modules/azure-devops-extension-sdk",
            "addressable": true,
            "packagePath": "lib"
        }
    ]
}