{
  "manifestVersion": 1,
  "id": "pr-tabs-build",
  "publisher": "fededim",
  "version": "1.0.5",
  "name": "Pull Request Builds tab",
  "description": "An extension to add a \"Builds\" tab to pull requests showing all the related builds generated by every commit",
  "public": true,
  "categories": [ "Azure Repos" ],
  "targets": [
    {
      "id": "Microsoft.VisualStudio.Services"
    }
  ],
  "galleryFlags": [
    "Public"
  ],
  "contributions": [
    {
      "id": "pr-tabs-build",
      "type": "ms.vss-web.tab",
      "targets": [
        "ms.vss-code-web.pr-tabs"
      ],
      "properties": {
        "name": "Builds",
        "uri": "pr-tabs-build/pr-tabs-build.html"
      }
    }
  ],
  "demands": [
    "api-version/3.0"
  ],
  "scopes": [ "vso.build", "vso.code" ],
  "repository": {
    "type": "git",
    "uri": "https://github.com/fededim/PullRequestBuildTab"
  },
  "content": {
    "details": {
      "path": "README.md"
    },
    "license": {
      "path": "LICENSE.md"
    }
  },
  "tags": [
    "pull requests",
    "build",
    "link",
    "tab"
  ],
  "links": {
    "home": {
      "uri": "https://fededim.github.io"
    },
    "repository": {
      "uri": "https://github.com/fededim/PullRequestBuildTab"
    },
    "issues": {
      "uri": "https://github.com/fededim/PullRequestBuildTab/issues"
    }
  },
  "icons": {
    "default": "static/images/logo.png"
  },
  "files": [
    {
      "path": "static",
      "addressable": true
    },
    {
      "path": "dist",
      "packagePath": "/",
      "addressable": true
    },
    {
      "path": "node_modules/azure-devops-extension-sdk",
      "addressable": true,
      "packagePath": "lib"
    }
  ],
  "branding": {
    "color": "rgb(25, 116, 210)",
    "theme": "light"
  },
  "screenshots": [
    {
      "path": "static/screenshots/screen1.jpg"
    },
    {
      "path": "static/screenshots/screen2.jpg"
    },
    {
      "path": "static/screenshots/screen4.jpg"
    },
    {
      "path": "static/screenshots/screen3.jpg"
    }
  ]
}
